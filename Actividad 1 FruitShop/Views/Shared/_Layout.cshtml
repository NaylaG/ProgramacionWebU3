@inject Actividad_1_FruitShop.Services.CategoriaService servicio
<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>FruitStore - La mejor fruteria de la Web</title>
    <link href="~/styles.css?fecha=hoy" rel="stylesheet" />
</head>
<body>
    <header></header>
    <nav>
        <a href="~/">Principal</a>
        @foreach (var item in servicio.Categorias)
        {
            <a href="~/@item.Nombre">@item.Nombre</a>
        }


    </nav>
    <main>
        @RenderBody()
    </main>
</body>
</html>